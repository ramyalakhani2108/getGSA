    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
